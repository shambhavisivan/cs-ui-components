{
	"name": "@cloudsense/cs-grid",
	"version": "1.2.62",
	"description": "CloudSense Grid",
	"main": "./lib/main.js",
	"author": "CloudSense",
	"repository": {
		"type": "git",
		"url": "https://gerrit.cloudsenseltd.com/gerrit/cs-ui-components"
	},
	"scripts": {
		"build": "npm run empty_lib && npm run move_fonts && npm run scss && tsc",
		"build-full": "npm run empty_lib && npm run move_fonts && npm run lint && npm run test && npm run scss && tsc && npm run empty_lib_test",
		"prepublishOnly": "npm run build-full",
		"scss": "node-sass ./src/sass/style.scss ./css/styles.css",
		"start": "npm run lint && webpack-dev-server --content-base src/ --config config/webpack.development.config.js --mode development --open",
		"build-demo": "rimraf ./cs-grid-demo && webpack --mode development --config config/webpack.demo.config.js",
		"lint": "tslint 'src/**/*.ts?(x)' --config ./tslint.json --project ./tsconfig.json --fix",
		"prettier": "prettier --check 'src/**/*.ts?(x)' --write",
		"empty_lib": "rimraf ./lib/*",
		"empty_lib_test": "rimraf ./lib/test && rimraf ./lib/src/index.js && rimraf ./lib/src/index.d.ts",
		"move_fonts": "ncp ./src/fonts/ ./fonts/",
		"clearJest": "npx jest --clearCache",
		"test": "npm run clearJest && cross-env NODE_ICU_DATA=node_modules/full-icu jest --verbose"
	},
	"private": false,
	"license": "UNLICENSED",
	"files": [
		"lib",
		"css",
		"fonts"
	],
	"types": "./lib/main.d.ts",
	"eslintConfig": {
		"extends": "react-app"
	},
	"keywords": [
		"grid",
		"data",
		"table",
		"typescript",
		"react",
		"react-component",
		"reactjs",
		"ui",
		"cs-ui-components",
		"ag-grid"
	],
	"dependencies": {
		"@cloudsense/react-datepicker-dynamiclocale": "2.0.1",
		"ag-grid-community": "26.2.1",
		"ag-grid-react": "26.2.0",
		"jsdom": "^16.1.0",
		"keycode-js": "^3.1.0",
		"lodash": "4.17.21",
		"moment": "^2.29.1"
	},
	"devDependencies": {
		"@babel/core": "7.16.7",
		"@babel/plugin-proposal-class-properties": "7.16.7",
		"@babel/plugin-proposal-function-bind": "7.16.7",
		"@babel/preset-env": "^7.16.7",
		"@babel/preset-react": "7.16.7",
		"@cloudsense/cs-ui-components": "2.0.0-alpha1",
		"@types/classnames": "^2.3.0",
		"@types/enzyme": "^3.10.11",
		"@types/jest": "^24.0.19",
		"@types/jsdom": "^12.2.4",
		"@types/lodash": "^4.14.178",
		"@types/react": "^17.0.38",
		"@types/react-datepicker": "^4.3.4",
		"@types/react-dom": "^17.0.11",
		"@types/webpack-env": "^1.13.9",
		"@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
		"awesome-typescript-loader": "^5.2.1",
		"babel-loader": "^8.2.3",
		"cross-env": "^7.0.3",
		"crypto": "^1.0.1",
		"css-loader": "^2.1.1",
		"enzyme": "^3.11.0",
		"file-loader": "^6.2.0",
		"full-icu": "^1.4.0",
		"html-webpack-plugin": "^3.2.0",
		"image-webpack-loader": "^4.6.0",
		"jest": "^26.6.3",
		"jest-environment-jsdom-sixteen": "^1.0.3",
		"mini-css-extract-plugin": "^0.5.0",
		"mkdirp": "^1.0.4",
		"ncp": "^2.0.0",
		"node-sass": "^4.14.1",
		"prettier": "^2.5.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-svg-loader": "^3.0.3",
		"rimraf": "^3.0.2",
		"sass-loader": "^7.1.0",
		"source-map-loader": "^0.2.4",
		"style-loader": "^0.23.1",
		"ts-jest": "^26.4.4",
		"tslint": "^5.20.1",
		"tslint-config-prettier": "^1.18.0",
		"tslint-plugin-prettier": "^2.3.0",
		"tslint-react": "^4.1.0",
		"typescript": "3.9.5",
		"url-loader": "^1.1.2",
		"webpack": "^4.39.3",
		"webpack-cli": "^3.3.11",
		"webpack-dev-server": "^3.11.0"
	},
	"peerDependencies": {
		"@cloudsense/cs-ui-components": "1.1.26-alpha6",
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	}
}
