{
	"name": "@cloudsense/cs-grid",
	"version": "1.0.34",
	"description": "CloudSense Grid",
	"main": "./lib/main.js",
	"author": "CloudSense",
	"repository": {
		"type": "git",
		"url": "https://gerrit.cloudsenseltd.com/gerrit/cs-ui-components"
	},
	"scripts": {
		"build": "rm -rf ./lib/* && cp -a ./src/fonts/ ./fonts/ && npm run scss && tsc",
		"prepare": "npm run build",
		"scss": "node-sass ./src/sass/style.scss ./css/styles.css",
		"test": "./test.sh",
		"start": "npm run lint && webpack-dev-server --content-base src/ --config config/webpack.development.config.js --mode development --open",
		"build-demo": "rm -rf ./cs-grid-demo && webpack --mode development --config config/webpack.demo.config.js",
		"lint": "tslint 'src/**/*.ts?(x)' --config ./tslint.json --project ./tsconfig.json --fix",
		"prettier": "prettier --check 'src/**/*.ts?(x)' --write"
	},
	"private": false,
	"license": "UNLICENSED",
	"files": [
		"lib",
		"css",
		"fonts"
	],
	"types": "./lib/main.d.ts",
	"eslintConfig": {
		"extends": "react-app"
	},
	"dependencies": {
		"@types/intl": "^1.2.0",
		"@types/react": "^16.8.23",
		"@types/react-datepicker": "^2.8.1",
		"@types/react-dom": "^16.8.4",
		"ag-grid-community": "~21.0.1",
		"ag-grid-react": "~21.0.1",
		"intl": "^1.2.5",
		"moment": "^2.24.0",
		"react": "^16.8.6",
		"react-datepicker": "^2.8.0",
		"react-dom": "^16.8.6",
		"typescript": "^3.5.3"
	},
	"devDependencies": {
		"@babel/core": "7.4.4",
		"@babel/plugin-proposal-class-properties": "7.4.4",
		"@babel/plugin-proposal-function-bind": "7.2.0",
		"@babel/preset-env": "^7.5.5",
		"@babel/preset-react": "7.0.0",
		"@types/webpack-env": "^1.13.9",
		"awesome-typescript-loader": "^5.2.1",
		"babel-loader": "^8.0.6",
		"css-loader": "^2.1.1",
		"file-loader": "^3.0.1",
		"html-webpack-plugin": "^3.2.0",
		"image-webpack-loader": "^4.6.0",
		"mini-css-extract-plugin": "^0.5.0",
		"mkdirp": "^0.5.1",
