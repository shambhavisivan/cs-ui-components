		"source-map-loader": "^0.2.4",
		"style-loader": "^0.23.1",
		"tslint": "^5.18.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-plugin-prettier": "^2.0.1",
		"tslint-react": "^4.0.0",
		"url-loader": "^1.1.2",
		"webpack": "^4.39.3",
		"webpack-cli": "3.3.0",
		"webpack-dev-server": "^3.2.1"
	}
